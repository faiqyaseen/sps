<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spsproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/tpo_login_img"
        android:label="@string/app_name"
        android:roundIcon="@drawable/tpo_login_img"
        android:supportsRtl="true"
        android:theme="@style/Theme.SpsProject">
        <activity android:name=".AddSelectedStudent"></activity>
        <activity android:name=".AllSelectedStudents" />
        <activity android:name=".AddJobDescription" />
        <activity android:name=".AllJobDescriptions" />
        <activity android:name=".AddCompany" />
        <activity android:name=".AllCompanies" />
        <activity android:name=".TpoDashboard" />
        <activity android:name=".AllStudents" />
        <activity android:name=".AllTpos" />
        <activity android:name=".AddStudent" />
        <activity android:name=".AddTpo" />
        <activity android:name=".TpoLogin" />
        <activity android:name=".StudentLogin" />
        <activity android:name=".AdminDashboard" />
        <activity android:name=".AdminLogin" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SplashScreen"
            android:noHistory="true"
            android:theme="@style/splash_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>